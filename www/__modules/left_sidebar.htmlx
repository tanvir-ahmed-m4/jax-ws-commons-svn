<script>
var info = {
	title: "JAX-WS commons",
	logo: "http://jax-ws.java.net/images/javaxml-duke.gif"
};

// select the right menu
(function() {
  function entry2(id,prefix) {
    var e = document.getElementById(id);
    return { element: e, url: "http://jax-ws-commons.java.net/"+prefix };
  }
  function entry(name) {
    return entry2("menubar-"+name,name+"/");
  }

  var menus = [
    entry2("menubar-main",""),
    entry("spring"),
    entry("in-vm"),
    entry("http-session-scope"),
    entry("thread-scope"),
    entry("json"),
    entry("dime"),
    entry("jaxws-maven-plugin"),
    entry("beans-generator"),
    entry("virtualbox"),
    entry("ec2"),
    entry("guice"),
    entry("grails"),
    entry("smtp")
  ];

  var activeMenu = menus[0];
  for( var i=1; i<menus.length; i++ ) {
    if(window.location.href.indexOf(menus[i].url)==0)
      activeMenu = menus[i];
  }
  
  for( var i=0; i<menus.length; i++ ) {
    menus[i].element.style.display = menus[i]==activeMenu ? "" : "none";
  }
})();
</script>
